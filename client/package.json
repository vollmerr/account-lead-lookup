  "devDependencies": {
    "bpa-azure-test": "0.0.1",
    "env-cmd": "^8.0.2",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "whatwg-fetch": "^3.0.0"
  }
}
